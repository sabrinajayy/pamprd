            <div class="artist-calendar">
                      <div id="bookings">
          <!-- <#% raise %> -->
                <%#= link_to "<", date: @date.prev_month %>
                <h2 id='month'><%= @date.strftime("%B %Y") %></h2>
                <%#= link_to ">", date: @date.next_month %>

                <%= calendar @date do |date| %>
                  <%= date.day %>
                  <% if @time_blocks_by_date %>
                  <% if @time_blocks_by_date[date.to_date] %>
                  <ul>
                    <% @time_blocks_by_date[date.to_date].each do |timeblock| %>
                      <li><%= timeblock.pretty_time %> - <%= link_to timeblock.consumer_name, consumer_consumers_events_path(timeblock.consumer_request)  %></li>

                      <% end %>
                  </ul>
                  <% end %>

                <% end %>
                <% end %>
              </div>
