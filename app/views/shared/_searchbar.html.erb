<%= form_tag('/search_results', method: 'get') do  %>
     <%#= text_field_tag :category, :as => :select, :collection => Artist::CATEGORY, placeholder: 'Choose a service', class: 'search-form' %>
     <%= select_tag 'category', options_for_select([['Hair', 'hair'], ['Makeup', 'makeup'], ['Hair and Makeup', 'both']]),  placeholder: 'Category', class: 'search-form' %>
     <%= text_field_tag :date, nil, placeholder: 'Date', class: 'search-form' %>
     <%= text_field_tag :time, nil, placeholder: 'Time', class: 'search-form' %>
     <%= text_field_tag :location, nil, placeholder: 'City', class: 'search-form' %>
     <%= submit_tag 'search', class: "btn-pamprd" %>
<% end  %>
